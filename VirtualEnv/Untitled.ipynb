   "source": [
    "from scipy.stats import norm\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "da7390f0-15b1-49e4-9540-4fcdcf7e2fee",
   "metadata": {},
   "outputs": [],
   "source": [
    "n = 100\n",
    "p = 1/3\n",
    "mu = n*p\n",
    "sig = np.sqrt(n * p * (1-p))\n",
    "Xn = 69.5"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "89343403-70c4-4095-9c5b-05f8061c4e44",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
